<h3>Contact</h3>

<div class="card z-depth-3">
	
	<div class="card-content">
		<p>Come find me, and let's have a cup of tea</p>
		<div id="map"></div>
	</div>
</div>

<style>
      #map {
        height: 400px;
        width: 100%;
       }
    </style>
  </head>
  <body>
    <div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: 34.0736, lng: -118.4004};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 15,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>

    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDqQcE57pmO3HBHNpqOb14N1WC4p69qcnk
&callback=initMap">
    </script>
  </body>
</html>